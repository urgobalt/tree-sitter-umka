{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "umka",
      "camelcase": "Umka",
      "title": "Umka grammar for tree-sitter",
      "scope": "source.umka",
      "injection-regex": "^umka$",
      "path": ".",
      "file-types": [
        "um"
      ],
      "highlights": "queries/umka/highlights.scm",
      "locals": "queries/umka/locals.scm",
      "class-name": "TreeSitterUmka"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "description": "Umka grammar for tree-sitter",
    "license": "MIT",
    "links": {
      "repository": "https://github.com/thacuber2a03/tree-sitter-umka"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
